{
  "name": "signiflyscoreboard",
  "description": "",
  "author": "Reh Ernest",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "app/index.html",
  "scripts": {
    "clean": "rimraf dist .cache",
    "dev": "parcel",
    "build": "parcel build"
  },
  "packageManager": "yarn@3.0.2",
  "devDependencies": {
    "parcel": "^2.0.0-rc.0"
  },
  "dependencies": {
    "@parcel/transformer-sass": "^2.8.2",
    "parcel-plugin-asset-copier": "^1.1.0"
  },
  "assetsPath": "assets/img",
  "parcel-plugin-structurize": {
    "rules": [
      {
        "match": "index.html",
        "folder": "app"
      },
      {
        "match": "*.html",
        "folder": "app"
      },
      {
        "match": "*.css",
        "folder": "app/assets/css"
      },
      {
        "match": "*.{eot,ttf,woff,woff2}",
        "folder": "app/assets/fonts"
      },
      {
        "match": "*.{jpg,jpeg,png,gif,svg,webp}",
        "folder": "app/assets/img"
      },
      {
        "match": "*.js",
        "folder": "app/assets/js"
      }
    ]
  }
}